
<div class="row">
  <div class="col s12 m6 l6">
    <ul class="collection with-header">
      <li class="collection-header"><h4>All Cities</h4></li>
      <% @cities.each do |city| %>
      <li class="collection-item"><%= link_to_if current_user.city == city, city.name, city_path(city) %></li>
      <% end %>
    </ul>
  </div>
</div>
